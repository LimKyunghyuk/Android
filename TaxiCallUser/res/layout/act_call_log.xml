<?xml version="1.0" encoding="utf-8"?>

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_height="fill_parent" android:layout_width="fill_parent" android:orientation="vertical">
	xmlns:android="http://schemas.android.com/apk/res/android"
	<LinearLayout android:layout_height="wrap_content" android:layout_width="fill_parent" android:layout_weight="0" android:gravity="center" android:background="#000">
<TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/app_name" style="@style/TextTitle" android:id="@+id/txtCallLogTitle"></TextView>
</LinearLayout><LinearLayout android:layout_height="wrap_content" android:layout_width="fill_parent" android:layout_weight="0" android:gravity="center">
<RelativeLayout android:layout_height="fill_parent" android:layout_width="fill_parent"><com.telcoware.taxicalluser.CVHeader android:layout_width="fill_parent" android:layout_centerInParent="true" android:layout_height="30dp"></com.telcoware.taxicalluser.CVHeader><TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_centerInParent="true" style="@style/TextHead" android:id="@+id/txtCallLogHeader" android:text="이력보기"></TextView>
</RelativeLayout>


</LinearLayout>
<TextView android:layout_height="wrap_content" android:id="@+id/txtCallogEmpty" android:background="#ffffffff" android:layout_width="fill_parent" android:textColor="#000000"></TextView>

<ListView android:layout_width="fill_parent"
		android:layout_height="fill_parent" android:background="#fff"
		android:id="@+id/listCallLog">
	</ListView>
</LinearLayout>