<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="fill_parent">

	<TextView android:id="@+id/TextView01" android:layout_width="wrap_content"
		android:layout_height="wrap_content"></TextView>
	<ImageView android:id="@+id/ImageView01"
		android:layout_height="100dip" android:layout_width="100dip"></ImageView>
	<RatingBar style="?android:attr/ratingBarStyleSmall" android:id="@+id/ratingbar"
		android:layout_gravity="center" android:numStars="5" android:stepSize="0.5"
		android:rating="2.5" android:focusable="true"
		android:focusableInTouchMode="false" android:soundEffectsEnabled="true"
		android:layout_height="wrap_content" android:layout_width="wrap_content" />

</LinearLayout>
